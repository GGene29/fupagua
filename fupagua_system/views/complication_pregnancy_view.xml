<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- View Tree -->
        <record model="ir.ui.view" id="complication_pregnancy_view_tree">
            <field name="name"> complication pregnancy</field>
            <field name="model">complication_pregnancy</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name" />
                </tree>
            </field>
        </record>

        <!-- View Form -->
        <record model="ir.ui.view" id="complication_pregnancy_view_form">
            <field name="name"> complication pregnancy</field>
            <field name="model">complication_pregnancy</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name" />
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <!-- Action Server View-->
        
        <record id="complication_pregnancy_view_action" model="ir.actions.act_window">
            <field name="name">complication pregnancy</field>
            <field name="res_model">complication_pregnancy</field>
            <field name="view_mode">tree,form</field>
        </record>
    
    </data>
</odoo>