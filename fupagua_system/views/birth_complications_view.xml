<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>

        <!-- View Tree -->
        <record model="ir.ui.view" id="birth_complications_view">
            <field name="name">birth_complications</field>    
            <field name="model">birth_complications</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name" />
                </tree>
            </field>
        </record>

    <!-- View Form -->
        <record model="ir.ui.view" id="birth_complications_form">
            <field name="name">birth_complications</field>    
            <field name="model">birth_complications</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name" />    
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

    <record id="birth_complications_view_action" model="ir.actions.act_window">
        <field name="name">Birth Complication</field>
        <field name="res_model">birth_complications</field>
        <field name="view_mode">tree,form</field>
    </record>
    
    </data>
</odoo>