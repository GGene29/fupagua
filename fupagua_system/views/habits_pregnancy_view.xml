<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- View Tree -->
        <record model="ir.ui.view" id="habits_pregnancy_view_tree">
            <field name="name"> habits pregnancy </field>
            <field name="model">habits_pregnancy</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name" />
                </tree>
            </field>
        </record>

        <!-- View Form -->
        <record model="ir.ui.view" id="habits_pregnancy_view_">
            <field name="name"> habits pregnancy</field>
            <field name="model">habits_pregnancy</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name" />                
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <!-- Action Server View-->
        <record model="ir.actions.server" id="habits_pregnancy_action_server">
            <field name="name">habits_pregnancy</field>
            <field name="model_id" ref="model_habits_pregnancy"/>
            <field name="state">code</field>
            <field name="code">
              action = {
                "type": "ir.actions.act_window",
                "view_mode": "tree,form",
                "res_model": model._name,
              }
            </field>
        </record>

        <!-- Menu -->
        <menuitem name="Habits Pregnancy" id="habits_pregnancy_type_menu" action="habits_pregnancy_action_server" sequence="110" />

    </data>
</odoo>